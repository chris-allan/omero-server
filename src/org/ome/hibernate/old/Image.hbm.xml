<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.ome.hibernate.old">
	<class name="Image">
		<id name="id" column="imageId">
			<generator class="native"/>
		</id>
		<set name="datasets" inverse="true" lazy="true" cascade="save-update">
			<key column="imageId"/>
			<many-to-many column="datasetId" class="Dataset"/>
		</set>
	</class>
</hibernate-mapping>